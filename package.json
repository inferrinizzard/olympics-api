{
	"name": "olympics-api",
	"version": "1.0.0",
	"description": "",
	"main": "src/index.ts",
	"exports": "./src/index.ts",
	"type": "module",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/inferrinizzard/olympics-api.git"
	},
	"author": "inferrinizzard",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/inferrinizzard/olympics-api/issues"
	},
	"homepage": "https://github.com/inferrinizzard/olympics-api#readme",
	"engines": {
		"node": "^14.13.1 || >=16.0.0"
	},
	"scripts": {
		"clean": "rm -r dist",
		"dev": "tsoa routes-and-spec && nodemon src/index.ts",
		"build": "tsoa routes-and-spec && tsc",
		"start": "node dist/index.js"
	},
	"nodemonConfig": {
		"restartable": "rs",
		"ignore": [
			".git",
			"node_modules/**/node_modules",
			"lib/**/*"
		],
		"execMap": {
			"ts": "ts-node --esm --experimentalSpecifierResolution node"
		},
		"ext": "ts,js,json"
	},
	"dependencies": {
		"dotenv": "^16.0.0",
		"express": "^4.17.3",
		"got": "^12.0.3",
		"jsdom": "^19.0.0",
		"pg-promise": "^10.11.1",
		"swagger-ui-express": "^4.4.0",
		"tsoa": "^4.1.0"
	},
	"devDependencies": {
		"@types/express": "^4.17.13",
		"@types/jsdom": "^16.2.14",
		"@types/multer": "^1.4.7",
		"@types/node": "^17.0.31",
		"@types/pg-promise": "^5.4.3",
		"@types/swagger-ui-express": "^4.1.3",
		"multer": "^1.4.5-lts.1",
		"nodemon": "^2.0.15",
		"ts-node": "^10.8.2",
		"typescript": "^4.6.3"
	}
}
